{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "prettier/prettier": "error"
  }
}
